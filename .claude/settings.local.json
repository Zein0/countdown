{
  "permissions": {
    "allow": [
      "Bash(npm start)",
      "Bash(npx expo start:*)",
      "Bash(taskkill:*)",
      "Bash(if exist node_modules rmdir /s /q node_modules)",
      "Bash(if exist package-lock.json del /f /q package-lock.json)",
      "Bash(if exist .expo rmdir /s /q .expo)",
      "Bash(rm:*)",
      "Bash(npm install)",
      "Bash(npm install:*)",
      "Bash(cat:*)"
    ],
    "deny": [],
    "ask": []
  }
}
